<script>
    jQuery('.tab').click(function() {
        jQuery(this).addClass('active').siblings().removeClass('active');
    });

    jQuery(".header-main-menu-bar").click(function() {
        jQuery(".header-main-menu-bar").toggleClass('open');
        jQuery(".menu-section").addClass("modal-is-visible");
        if (jQuery('body').hasClass('open-menu')) jQuery('body').removeClass('open-menu');
        else
            jQuery('body').addClass('open-menu');
    });
    jQuery(".menu-close-btn").click(function() {
        jQuery(".menu-section").removeClass("modal-is-visible");
        if (jQuery('body').hasClass('open-menu')) jQuery('body').removeClass('open-menu');
        else
            jQuery('body').addClass('open-menu');
    });
    jQuery(".inqry").click(function() {
        jQuery(".enquiry-section_part").fadeIn();
        if (jQuery('body').hasClass('open-enquiry')) {
            jQuery("#gform_wrapper_3").removeClass("openform");
            jQuery('body').removeClass('open-enquiry');
        }
    });
    jQuery(".enquiry-close-btn").click(function(){
        jQuery(".enquiry-section_part").slideToggle("slow");
    });
    jQuery("#file_input").change(function() {
        var name;
        this.files && this.files.length ? name = this.files[0].name.split('.')[0] : name = 'Upload your CV';
        jQuery('#fileName').text(name);
    });
    jQuery('.multi-item-carousel').carousel({
      interval: false
    });

</script>

